function mainTesting(){
	var idx = 0;
	var timer = setInterval(function(){
		idx++;
		if($('.jsTesting').not('.slick-initialized').length > 0){
			$('.jsTesting').not('.slick-initialized').slick({
				infinite:true,
				speed:300,
				slidesToShow:5,
				slidesToScroll:5,
				prevArrow:'<button type="button" class="slick-prev"><span>Previous</span></button>',
				nextArrow:'<button type="button" class="slick-next"><span>Next</span></button>',
				responsive:[
					{
					breakpoint:1770,
					settings:{
							slidesToShow:3,
							slidesToScroll:3
						}
					}
				]
			});
			clearInterval(timer);
		}else if(idx > 5){
			clearInterval(timer);
		}
	},500)
}

